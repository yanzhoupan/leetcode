# You can sell and buy stocks within the same day.
class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        sumProfit = 0
        if len(prices)<=1:
        	return sumProfit
        for i in range(1, len(prices)):
        	diff = prices[i] - prices[i-1]
        	if diff>0:
        		sumProfit += diff
        return sumProfit
        
