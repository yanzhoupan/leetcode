class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        # inter = []
        # for i in nums1:
        #     if i in nums2 and i not in inter:
        #         inter.append(i)
        # return inter
        
        # 大佬解法：
        return list(set(nums1) & set(nums2))
