class Solution(object):
    def isUgly(self, num):
        """
        :type num: int
        :rtype: bool
        """
        if num<1:
            return False
        while num%2 == 0 and num > 0:
            num = num/2
        while num%3 == 0 and num > 0:
            num = num/3
        while num%5 == 0 and num > 0:
            num = num/5
        if num == 1:
            return True
        return False
        
# 依次剥离因子
